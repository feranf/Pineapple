def .main
    let p = new People
        :name = "Wong"
        :age  = 25
    
    let q = new People
        :name = "Johnny"
        :age  = 25

    if not p.isOld and q.isOld
        "p is not old man".show
    else
        "p is old".show

def People
    :name String
    :age  Integer


def (this People).isOld -> Boolean
    return this:age > 20
