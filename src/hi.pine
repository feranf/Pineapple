def .main
    let p = People
        :name = "Wong"
        :age  = 25
    
    let q = People
        :name = "Johnny"
        :age  = 25

    if not p.isOld and q.isOld
        "p is not old man".show
    elif p.isBetterThan(q)
        "p is old".show

def People
    :name String
    :age  Integer


def (this People).isOld -> Boolean
    return this:age > 20

def (this People).isBetterThan(that People) -> Boolean
    return this:name.length > that:name.length